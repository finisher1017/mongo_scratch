<h1>Listing Deskphones</h1>
 <p><%= link_to 'New Deskphone', new_deskphone_path %></p>
<table>
  <tr>
    <th>Manufacturer</th>
    <th>Model</th>
    <th>Jslgtag</th>
    <th>Location</th>
    <th>Assignedto</th>
    <th>Status</th>
    <th colspan="3"></th>
  </tr>

<tbody> 
  <% @deskphone.each do |deskphone| %>
    <tr>
      <td><%= deskphone.manufacturer %></td>
      <td><%= deskphone.equipmentModel %></td>
      <td><%= deskphone.jslgTag %></td>
        <td><%= deskphone.location %></td>
        <td><%= deskphone.assigneeName %></td>
        <td><%= deskphone.status %></td>
        <td><%= link_to 'Show', deskphone_path(deskphone) %></td>
        <td><%= link_to 'Edit', edit_deskphone_path(deskphone) %></td>
        <td><%= link_to 'Destroy', deskphone_path(deskphone),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
<tbody>
</table>
